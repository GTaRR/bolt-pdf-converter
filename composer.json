{
    "name": "gtarr/pdfrenderer",
    "description": "Bolt extension for render PDF to JPG images on server.",
    "type": "bolt-extension",
    "keywords": ["pdf"],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Sushilov",
            "email": "mr.sushiloff@yandex.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\GTaRR\\PdfRenderer\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\GTaRR\\PdfRenderer\\PdfRendererExtension"
    }
}
